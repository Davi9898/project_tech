<div class="container">
  <div class="row2">
    <h1> Login </h1>

    <%- include ('./partials/messages') %>

      <form action="/users/login" method="POST">
        <label for="email">Email</label>
        <input type="email" name="email" class="formlogin" placeholder="Enter Email" />


        <label for="password">Password</label>
        <input type="password" name="password" class="formlogin" placeholder="Enter Password" />

        <button type="submit">Login</button>
      </form>

      <a href="/users/register">Register</a>
  </div>
</div>